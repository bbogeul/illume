,\n  template: 'about'\n  layoutTemplate: 'appLayout'\n  path: '/about-berguerpr'\n  title: 'About | Berguer PR'\n  meta:\n    url:\n      property: 'og:url'\n      itemprop: 'url'\n      content: ' '\nRouter.route 'service',\n  template: 'service'\n  layoutTemplate: 'appLayout'\n  path: '/our-services'\n  title: 'Services | Berguer PR'\n  meta:\n    url:\n      property: 'og:url'\n      itemprop: 'url'\n      content: ' ' \nRouter.route 'applications',\n  template: 'applications'\n  layoutTemplate: 'appLayout'\n   \n  path: '/work-with-us'\n  title: 'Work with Berguer PR'\n  meta:\n    url:\n      property: 'og:url'\n      itemprop: 'url'\n      content: ' '\n \nRouter.route 'rsvp',\n  template: 'rsvp'\n  controller: 'RsvpController',\n  action: 'action',\n  where: 'client'\n \nRouter.route 'dashboards',\n  template: 'dashboards'\n  layoutTemplate: 'loginLayout'\n  path: '/dashboard'\n  title: 'Dashboard'\n  meta:\n    url:\n      property: 'og:url'\n      itemprop: 'url'\n      content: ' '\nRouter.route 'register',\n  template: 'register'\n  layoutTemplate: 'registerTemplate'\n  path: '/register'\n  title: 'Register'\n  meta:\n    url:\n      property: 'og:url'\n      itemprop: 'url'\n      content: ' '   "," \nif (Meteor.isClient) {\n  // This code only runs on the client\n  \n Template.application.events({\n    \n    \"click .delete\": function () {\n      Rsvps.remove(this._id);\n    }\n  });\n}\n\n\n \n ","if (Meteor.isClient) {\n  Template.dashboard.events({\n    'click .logout': function(event){\n        event.preventDefault();\n        Meteor.logout();\n    }\n  });\n  Template.register.events({\n    'submit form': function(event){\n        event.preventDefault();\n        var emailVar = event.target.registerEmail.value;\n        var passwordVar = event.target.registerPassword.value;\n        Accounts.createUser({\n            email: emailVar,\n            password: passwordVar\n        });\n    }\n  });\n  Template.login.events({\n    'submit form': function(event){\n        event.preventDefault();\n        var emailVar = event.target.loginEmail.value;\n        var passwordVar = event.target.loginPassword.value;\n        Meteor.loginWithPassword(emailVar, passwordVar);\n    }\n  });\n}","Rsvps = new Mongo.Collection(\"rsvps\");\n\nRsvps.attachSchema(new SimpleSchema({\n  name: {\n    type: String,\n    label: \"First Name *\",\n    optional: false, \n    min: 2,\n    max: 80    \n  },\n  last: {\n    type: String,\n    label: \"Last Name *\",\n    optional: false, \n    min: 2,\n    max: 80    \n  },\norganization: {\n    type: String,\n    label: \"Company Name *\",\n    optional: false, \n    min: 2,\n    max: 80    \n  },\n  website: {\n    type: String,\n    label: \"Company website *\",\n    optional: false, \n    min: 4,\n    max: 80    \n  },\n    email: {\n    type: String,\n    label: \"Email *\",\n    optional: false, \n    min: 5,\n    max: 80    \n  },\n  budget: {\n    type: String,\n    label: \"What is your current monthly PR/Marketing budget? *\",\n    optional: false, \n    min: 1,\n    max: 80    \n  },\n   phone: {\n    type: Number,\n    label: \"Phone *\",\n    optional: false \n  },\n  industry: {\n    type: String,\n    label: \"What industry/category is your company in? *\",\n    optional: false \n  },\n  Competitors: {\n    type: String,\n    label: \"Which companies do you consider your main competitors? *\",\n    optional: false \n  },\n  iadSense: {\n      type: String,\n      optional: false,\n     label: \"How did you hear about Berguer PR?\",\n      autoform: {\n\n        options: [\n            {\n               label: \"Prior Client\",\n               value: \"Prior Client\"\n            },\n            {\n               label: \"Referral\",\n               value: \"Referral\"\n            },\n            {\n               label: \"Our Website\",\n               value: \"Our Website\"\n            },\n             {\n               label: \"News Article\",\n               value: \"News Article\"\n            },\n             {\n               label: \"Other\",\n               value: \"Other\"\n            }\n         ]\n      }\n   },\n    length: {\n      type: String,\n      optional: false,\n     label: \"Length of engagement desired *\",\n      autoform: {\n\n        options: [\n            {\n               label: \"3 months, or less\",\n               value: \"3 months or less\"\n            },\n            {\n               label: \"4-6 months\",\n               value: \"4-6 months\"\n            },\n            {\n               label: \"7-12 months\",\n               value: \"7-12 months\"\n            },\n             {\n               label: \"Not sure\",\n               value: \"Not sure\"\n            } \n         ]\n      }\n   },\n     whatElse: {\n      type: String,\n      optional: true,\n      label: \"What else should we know?\",\n      min: 20,\n      max: 1000,\n      autoform: {\n         rows: 5\n      }\n   },\n   another: {\n    type: Boolean,\n    optional: false,\n    autoform: {\n     type: \"switch\",\n      trueLabel: \"Online\",\n      falseLabel: \"Offline\"\n    }\n  },\n   pr: {\n    type: String,\n    optional: true,\n    label: \"Full Service\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n   dss: {\n    type: String,\n    optional: true,\n    label: \"Full Service\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n   scs: {\n    type: String,\n    optional: true,\n    label: \"Full Service\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n   san: {\n    type: String,\n    optional: true,\n    label: \"Social Analytics\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n  sam: {\n    type: String,\n    optional: true,\n    label: \"Media Analytics\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n ba: {\n    type: String,\n    optional: true,\n    label: \"Brand Analysis\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n  mdev: {\n    type: String,\n    optional: true,\n    label: \"Messaging Development\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },  \n  cp: {\n    type: String,\n    optional: true,\n    label: \"Campaign and/or product launch support\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n  scm: {\n    type: String,\n    optional: true,\n    label: \"Social channel management\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n  adv: {\n    type: String,\n    optional: true,\n    label: \"Advertising and/or paid media campaigns\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n  es: {\n    type: String,\n    optional: true,\n    label: \"Events & Sponsorship\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n  ao: {\n    type: String,\n    optional: true,\n    label: \"Awards Opportunities\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n  se: {\n    type: String,\n    optional: true,\n    label: \"Speaking Engagements\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n  ar: {\n    type: String,\n    optional: true,\n    label: \"Analyst Relations\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n  prls: {\n    type: String,\n    optional: true,\n    label: \"Project Launch Support\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n   mr: {\n    type: String,\n    optional: true,\n    label: \"Media Relations\",\n    autoform: {\n      afFieldInput: {\n        type: \"boolean-checkbox\"\n      }\n    }\n  },\n     dev: {\n      type: String,\n      optional: true,\n     label: \"What is the development stage of your company or product?\",\n      autoform: {\n\n        options: [\n            {\n               label: \"Pre-Launch\",\n               value: \"Pre-Launch\"\n            },\n            {\n               label: \"Launched within last 12 months\",\n               value: \"Launched within last 12 months\"\n            },\n            {\n               label: \"In the market over 12 months\",\n               value: \"In the market over 12 months\"\n            }\n         ]\n      }\n   },\n  createdAt: {\n    type: Date,\n    autoValue: function () {\n      return new Date();\n    }\n  }\n}));\n\nRsvps.allow({\n  insert: function (userId, doc) {\n    return true;\n  },\n  update: function (userId, doc, fields, modifier) {\n    return true;\n  },\n  remove: function (userId, doc) {\n    return true;\n  }\n});","main2Controller = RouteController.extend({\n  subscriptions: function () {\n    // set up the subscriptions for the route and optionally\n    // wait on them like this:\n    //\n    this.subscribe('rsvps').wait();\n    //\n    // \"Waiting\" on a subscription does not block. Instead,\n    // the subscription handle is added to a reactive list\n    // and when all items in this list are ready, this.ready()\n    // returns true in any of your route functions.\n  },\n\n  data: function () {\n    return {\n      rsvps: Rsvps.find({}, { sort:{ createdAt: -1 }, limit:100 })\n    }\n    // return a global data context like this:\n    // Items.findOne({_id: this.params._id});\n  },\n\n  action: function () {\n    \n    // You can create as many action functions as you'd like.\n    // This is the primary function for running your route.\n    // Usually it just renders a template to a page. But it\n    // might also perform some conditional logic. Override\n    // the data context by providing it as an option in the\n    // last parameter.\n    this.render('application', { /* data: {} */});\n  }\n});\n","gaController = RouteController.extend({\n \n\n  action: function () {\n     \n    // You can create as many action functions as you'd like.\n    // This is the primary function for running your route.\n    // Usually it just renders a template to a page. But it\n    // might also perform some conditional logic. Override\n    // the data context by providing it as an option in the\n    // last parameter.\n    this.render('home', { /* data: {} */});\n    GAnalytics.pageview();\n  }\n});\ngaController2 = RouteController.extend({\n \n\n  action: function () {\n     \n    // You can create as many action functions as you'd like.\n    // This is the primary function for running your route.\n    // Usually it just renders a template to a page. But it\n    // might also perform some conditional logic. Override\n    // the data context by providing it as an option in the\n    // last parameter.\n    this.render('applications', { /* data: {} */});\n    GAnalytics.pageview();\n  }\n});\n\n","mainController = RouteController.extend({\n  subscriptions: function () {\n    // set up the subscriptions for the route and optionally\n    // wait on them like this:\n    //\n    this.subscribe('rsvps').wait();\n    //\n    // \"Waiting\" on a subscription does not block. Instead,\n    // the subscription handle is added to a reactive list\n    // and when all items in this list are ready, this.ready()\n    // returns true in any of your route functions.\n  },\n\n  data: function () {\n    return {\n      rsvps: Rsvps.find({}, { sort:{ createdAt: -1 }, limit:1 })\n    }\n    // return a global data context like this:\n    // Items.findOne({_id: this.params._id});\n  },\n\n  action: function () {\n    \n    // You can create as many action functions as you'd like.\n    // This is the primary function for running your route.\n    // Usually it just renders a template to a page. But it\n    // might also perform some conditional logic. Override\n    // the data context by providing it as an option in the\n    // last parameter.\n    this.render('applications', { /* data: {} */});\n  }\n});\n"," /*****************************************************************************/\n/* Server Only Methods */\n/*****************************************************************************/\nMeteor.methods({\n\t'submitRsvp': function (rsvp) {\n\t\tconsole.log('new RSVP:', rsvp);\n\t\tRsvps.insert(rsvp);\n\t}\n});\n/**\n * Meteor.publish('items', function (param1, param2) {\n *  this.ready();\n * });\n */\nMeteor.publish('rsvps', function () {\n  return Rsvps.find();\n});"]}}]